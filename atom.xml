---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Anyway.ABC</title>
	<link href="http://abc.anyway.design/atom.xml" rel="self"/>
	<link href="http://abc.anyway.design"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://abc.anyway.design</id>
	<author>
		<name>Anyway.FM</name>
		<email>hello@Anyway.FM</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://abc.anyway.design{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://abc.anyway.design{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
